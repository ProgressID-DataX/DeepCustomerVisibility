{
    "node": {
        "content": "data(name)",
        "shape": "rectangle",
        "width": "mapData(name.length, 10, 100, 100, 800)",
        "color": "black",
        "background-color": "#6DE",
        "text-valign": "center"
    },
    "node:selected": {
        "color": "#6DE",
        "background-color": "black"
    },
    "node.journey": {
        "color": "black",
        "background-color": "green"
    },
    "node.prediction": {
        "shape": "ellipse",
        "background-color": "#ccc"
    },
    "edge": {
        "curve-style": "bezier",
        "opacity": 0.666,
        "width": "mapData(persons, 0, 10000, 1, 10)",
        "target-arrow-shape": "triangle",
        "line-color": "#ccc",
        "target-arrow-color": "black"
    },
    "edge:selected": {
        "line-color": "#383",
        "target-arrow-color": "#383"
    },
    "edge.journey": {
        "line-color": "green",
        "target-arrow-color": "green",
        "opacity": 1,
        "z-index": 1
    },
    "edge.prediction": {
        "line-color": "green",
        "target-arrow-color": "green",
        "line-style": "dotted",
        "target-arrow-shape": "diamond",
        "opacity": 1,
        "z-index": 1
    },
    ".faded": {
        "opacity": 0.25,
        "text-opacity": 0
    }
}
