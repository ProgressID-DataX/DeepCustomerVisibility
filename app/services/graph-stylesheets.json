{
    "node": {
        "content": "data(name)",
        "shape": "rectangle",
        "width": "mapData(name.length, 10, 100, 100, 800)",
        "color": "white",
        "background-color": "#563D7C",
        "text-valign": "center"
    },
    "node.prediction": {
        "shape": "ellipse",
        "color": "black",
        "background-color": "#FBB4B3"
    },
    "node.journey": {
        "color": "black",
        "background-color": "#4FC08D"
    },
    "node:selected": {
        "color": "black",
        "background-color": "#FDDD9B"
    },
    "edge": {
        "curve-style": "bezier",
        "opacity": 0.666,
        "width": "mapData(customers, 0, 10000, 1, 10)",
        "target-arrow-shape": "triangle",
        "line-color": "#ccc",
        "target-arrow-color": "black",
        "font-weight": "bold"
    },
    "edge.journey": {
        "line-color": "#4FC08D",
        "target-arrow-color": "green",
        "opacity": 1,
        "z-index": 1
    },
    "edge.prediction": {
        "line-color": "#FBB4B3",
        "target-arrow-color": "#CB8483",
        "line-style": "dotted",
        "target-arrow-shape": "diamond",
        "opacity": 1,
        "z-index": 2
    },
    "edge:selected": {
        "line-color": "#FDDD9B",
        "target-arrow-color": "#CDAD6B"
    },
    ".faded": {
        "opacity": 0.25,
        "text-opacity": 0
    }
}
